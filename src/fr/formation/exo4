class ErreursMethodes {

  public static void main (String[] args) { 
    int i1 = methode1();
    int i2 = methode2();
    int i3 = methode3();
    int i4 = methode4();
    methode5();
    methode6();
    methode7();
    methode8();
  }

  // manque ()
  static int methode1 /*ici */ {
    int a = 0;
    System.out.println("Méthode 1");
    return a;
  }

 
  static int methode2 () {
    int a = 0;
    i1 = 10;  /* i1 non connu,, */
    System.out.println("Méthode 2");
    return a;
  }

  static int methode3 () {
    int a = 0;
    System.out.println("Méthode 3");
     /* doit avoir un return type int */
  }

  static int methode4 () {
    String a = "0";
    System.out.println("Méthode 4");
    return a;  /* return type mismatched doit etre int */
  }

  static void methode5 () {
    double a = 0;
    System.out.println("Méthode 5");
    return a;  /* method void ,, et non affectable a une variable au-dessus */
  }

  static  /* specifier un type retour */ methode6 () {
    double a = 0;
    System.out.println("Méthode 6");
    return a;
  }

  static void methode7 () {
    int a = 0;
    double b = 5.5;
    methodeSansErreur(a);  /* require 2 param int,double */
    System.out.println("Méthode 7");
  }

  static void methode8 () {
    int a = 0;
    String b = "5.5";
    methodeSansErreur(a, b);  /* require 2 param  de typeint,double */
    System.out.println("Méthode 8");
  }

  static void methodeSansErreur (int a, double b) {
    // Cette méthode ne fait rien du tout
  }
}
